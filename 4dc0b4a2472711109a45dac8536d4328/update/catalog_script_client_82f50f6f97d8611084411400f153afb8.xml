        <name>Get opening and closing time</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
   if (isLoading || newValue == '') {
      return;
   }

	var date = g_form.getReference('day', callback);
	
	function callback(date) {
		g_form.clearMessages();
		var opening_time = date.getValue('opening_time');
		var closing_time = date.getValue('closing_time');
		g_form.showFieldMsg('day', 'Opening hours: ' + opening_time + ' - ' + closing_time + '.')
	}
	

